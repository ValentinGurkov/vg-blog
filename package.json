{
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ValentinGurkov/vg-blog.git"
  },
  "bugs": "https://github.com/ValentinGurkov/vg-blog/issues",
  "authors": [
    "Valentin Gurkov"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "node server/",
    "build": "next build",
    "start": "NODE_ENV=production node server/",
    "start:win": "set NODE_ENV=production& node server/",
    "lint:js": "eslint --fix --ext .js pages components server containers lib api next.config.js",
    "lint:css": "stylelint --fix containers/**/*.scss components/**/*.scss",
    "lint": "yarn lint:js && yarn lint:css",
    "test": "NODE_ENV=test jest",
    "test:win": "set NODE_ENV=test& jest",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:win": "set BUNDLE_ANALYZE=both& next build",
    "analyze:win:server": "set BUNDLE_ANALYZE=server& next build",
    "analyze:win:browser": "set BUNDLE_ANALYZE=browser& next build"
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-root-import": "^6.4.1",
    "cacheable-response": "^1.6.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "google-maps-react": "^2.0.2",
    "helmet": "^3.20.0",
    "next": "^9.0.3",
    "next-offline": "^4.0.3",
    "next-optimized-images": "^2.5.3",
    "pretty-error": "^2.1.1",
    "prismic-javascript": "^2.1.1",
    "prismic-reactjs": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sitemap": "^3.2.2",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.1z4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-jsx": "^7.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.8.0",
    "jest-environment-node-debug": "^2.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.2.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
