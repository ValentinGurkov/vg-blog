  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "node server/",
    "build": "next build",
    "start": "NODE_ENV=production node server/",
    "start:win": "set NODE_ENV=production& node server/",
    "lint:js": "eslint --fix --ext .js pages components server containers lib api next.config.js",
    "lint:css": "stylelint --fix containers/**/*.scss components/**/*.scss",
    "lint": "yarn lint:js && yarn lint:css",
    "test": "NODE_ENV=test jest",
    "test:win": "set NODE_ENV=test& jest",
    "analyze": "ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true& next build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.4.4",
    "babel-plugin-root-import": "^6.5.0",
    "cacheable-response": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "google-maps-react": "^2.0.6",
    "helmet": "^3.23.3",
    "next": "^9.4.4",
    "next-offline": "^5.0.2",
    "next-optimized-images": "^2.6.2",
    "pretty-error": "^2.1.1",
    "prismic-javascript": "^3.0.2",
    "prismic-reactjs": "^1.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sitemap": "^6.2.0",
    "webpack-pwa-manifest": "^4.2.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
