    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "google-maps-react": "^2.0.2",
    "helmet": "^3.21.1",
    "next": "^9.0.7",
    "next-offline": "^4.0.6",
